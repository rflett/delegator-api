									"            },\r",
									"            \"last_name\": {\r",
									"                \"type\": \"string\"\r",
									"            },\r",
									"            \"role_name\": {\r",
									"                \"type\": \"string\"\r",
									"            }\r",
									"        }\r",
									"    };\r",
									"\r",
									"pm.test('Schema is valid', function() {\r",
									"    pm.expect(ajv.validate(schema, responseBody)).to.be.true;\r",
									"});"
