									"    ajv = new Ajv({logger: console}),\r",
									"    schema = {\r",
									"        \"properties\": {\r",
									"            \"org_id\": {\r",
									"                \"type\": \"integer\"\r",
									"            },\r",
									"            \"id\": {\r",
									"                \"type\": \"integer\"\r",
									"            },\r",
									"            \"email\": {\r",
									"                \"type\": \"string\"\r",
									"            },\r",
