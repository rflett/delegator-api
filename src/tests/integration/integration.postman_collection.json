									"var Ajv = require('ajv'),\r",
									"    ajv = new Ajv({logger: console}),\r",
									"    schema = {\r",
									"        \"properties\": {\r",
									"            \"org_id\": {\r",
									"                \"type\": \"integer\"\r",
									"            },\r",
									"            \"id\": {\r",
									"                \"type\": \"integer\"\r",
									"            },\r",
									"            \"email\": {\r",
									"                \"type\": \"string\"\r",
									"            },\r",
									"            \"first_name\": {\r",
									"                \"type\": \"string\"\r",
									"            },\r",
									"            \"last_name\": {\r",
									"                \"type\": \"string\"\r",
									"            },\r",
									"            \"role_name\": {\r",
									"                \"type\": \"string\"\r",
									"            }\r",
									"        }\r",
									"    };\r",
									"\r",
									"pm.test('Schema is valid', function() {\r",
									"    pm.expect(ajv.validate(schema, responseBody)).to.be.true;\r",
									"});"
								],
