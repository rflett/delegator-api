[
    {
        "name": "api",
        "image": "008492826001.dkr.ecr.ap-southeast-2.amazonaws.com/api:latest",
        "taskRoleArn": "arn:aws:iam::008492826001:role/afterburner-api-container",
        "memory": 512,
        "memoryReservation": 64,
        "essential": true,
        "portMappings": [
            {
                "hostPort": 5000,
                "containerPort": 5000,
                "protocol": "tcp"
            }
        ],
        "environment" : [
            { "name" : "APP_ENV", "value" : "Staging" }
        ],
        "logConfiguration": {
            "logDriver": "awslogs",
            "options": {
                "awslogs-group": "api",
                "awslogs-region": "ap-southeast-2",
                "awslogs-stream-prefix": "api"
            }
        }
    }
]
